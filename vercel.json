{
  "version": 2,
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/Build/(.*)",
      "dest": "/public/Build/$1"
    },
    {
      "src": "/TemplateData/(.*)",
      "dest": "/public/TemplateData/$1"
    },
    {
      "src": "/StreamingAssets/(.*)",
      "dest": "/public/StreamingAssets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cross-Origin-Embedder-Policy",
          "value": "require-corp"
        },
        {
          "key": "Cross-Origin-Opener-Policy",
          "value": "same-origin"
        }
      ]
    }
  ]
} 